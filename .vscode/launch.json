 {
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug WordWing",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/target/debug/WordWing",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "DASHSCOPE_API_KEY",
                    "value": ""
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "cargo: build",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}
